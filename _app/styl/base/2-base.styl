*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: SUD
	font-weight: 300
	color #000
	background-color #fff
	line-height: 1.56
	@media screen and (min-width 2295px)
		font-size: 20px
.wrapper
	overflow hidden
.container
	width 100%
	max-width 1200px
	position relative
	@media screen and (min-width 2295px)
		max-width 1400px
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display inline-block
	font-size: 18px
	text-align center
	margin 0 auto
	text-decoration none
	color: #fff
	min-width 270px
	background-color: #9D7D5C;
	border-radius: 2px;
	padding: 10px 30px
	transition: all .3s ease
	&:focus
		text-decoration none
		color #fff
	&:hover
		text-decoration none
		color #fff
		background-color: #D5AD85
.general-title
	font-size: 42px;
	font-weight: 600;
	margin-bottom: 30px
	text-transform uppercase
	&-white
		color #fff
	@media screen and (max-width 991px)
		font-size: 33px
	@media screen and (max-width 560px)
		font-size: 24px
.general-list
	li
		list-style: none
		padding-left: 47px
		font-size: 24px
		margin-bottom: 36px
		position relative
		&:before
			content: ''
			display block
			position absolute
			width 26px
			height 3px
			background-color: #9D7D5C
			top 18px
			left 0
	&-white
		li
			color #fff
	@media screen and (max-width 991px)
		li
			margin-bottom: 20px
	@media screen and (max-width 560px)
		li
			font-size: 14px
			padding-left: 25px
			&:before
				width 15px
				top 10px